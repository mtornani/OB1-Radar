{
  "buildCommand": "python run.py && python setup_web.py",
  "outputDirectory": "public",
  "installCommand": "pip install -r requirements.txt",
  "functions": {
    "api/**/*.py": {
      "runtime": "python3.9"
    }
  },
  "crons": [
    {
      "path": "/api/cron/daily",
      "schedule": "0 3 * * *"
    }
  ]
}
